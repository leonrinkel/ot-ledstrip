{
    "version": 4,
    "env": {
        "sdk": "${workspaceFolder}/sdk",
        "gcc": "${workspaceFolder}/tools/gcc-arm-none-eabi-9-2019-q4-major"
    },
    "configurations": [
        {
            "name": "pca10056/blank",
            "defines": [
                "APP_TIMER_V2",
                "APP_TIMER_V2_RTC1_ENABLED",
                "BOARD_PCA10056",
                "CONFIG_GPIO_AS_PINRESET",
                "ENABLE_FEM",
                "FLOAT_ABI_HARD",
                "MBEDTLS_CONFIG_FILE=\"nrf-config.h\"",
                "MBEDTLS_USER_CONFIG_FILE=\"nrf52840-mbedtls-config.h\"",
                "NRF52840_XXAA",
                "OPENTHREAD_CONFIG_ENABLE_BUILTIN_MBEDTLS=0",
                "OPENTHREAD_CONFIG_FILE=\"openthread-config-wrap.h\"",
                "OPENTHREAD_FTD=1",
                "USBD_POWER_DETECTION=1",
                "__HEAP_SIZE=0",
                "__STACK_SIZE=8192"
            ],
            "includePath": [
                "${workspaceFolder}/src", "${sdk}/**",
                "${workspaceFolder}/targets/pca10056/blank/config"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/src", "${sdk}/**",
                    "${workspaceFolder}/targets/pca10056/blank/config"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "${gcc}/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mabi=aapcs",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16"
            ],
            "forcedInclude": [
                "${workspaceFolder}/.vscode/undef.h"
            ]
        },
        {
            "name": "pca10059/mbr",
            "defines": [
                "APP_TIMER_V2",
                "APP_TIMER_V2_RTC1_ENABLED",
                "BOARD_PCA10059",
                "CONFIG_GPIO_AS_PINRESET",
                "ENABLE_FEM",
                "FLOAT_ABI_HARD",
                "MBEDTLS_CONFIG_FILE=\"nrf-config.h\"",
                "MBEDTLS_USER_CONFIG_FILE=\"nrf52840-mbedtls-config.h\"",
                "MBR_PRESENT",
                "NRF52840_XXAA",
                "OPENTHREAD_CONFIG_ENABLE_BUILTIN_MBEDTLS=0",
                "OPENTHREAD_CONFIG_FILE=\"openthread-config-wrap.h\"",
                "OPENTHREAD_FTD=1",
                "USBD_POWER_DETECTION=1",
                "__HEAP_SIZE=0",
                "__STACK_SIZE=8192"
            ],
            "includePath": [
                "${workspaceFolder}/src", "${sdk}/**",
                "${workspaceFolder}/targets/pca10059/mbr/config"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/src", "${sdk}/**",
                    "${workspaceFolder}/targets/pca10059/mbr/config"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "${gcc}/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mabi=aapcs",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16"
            ],
            "forcedInclude": [
                "${workspaceFolder}/.vscode/undef.h"
            ]
        },
        {
            "name": "pca10100/blank",
            "defines": [
                "APP_TIMER_V2",
                "APP_TIMER_V2_RTC1_ENABLED",
                "BOARD_PCA10100",
                "CONFIG_GPIO_AS_PINRESET",
                "ENABLE_FEM",
                "FLOAT_ABI_HARD",
                "MBEDTLS_CONFIG_FILE=\"mbedtls-config.h\"",
                "MBEDTLS_USER_CONFIG_FILE=\"nrf52833-mbedtls-config.h\"",
                "NRF52833_XXAA",
                "OPENTHREAD_CONFIG_ENABLE_BUILTIN_MBEDTLS=1",
                "OPENTHREAD_CONFIG_FILE=\"openthread-config-wrap.h\"",
                "OPENTHREAD_FTD=1",
                "USBD_POWER_DETECTION=1",
                "__HEAP_SIZE=0",
                "__STACK_SIZE=8192"
            ],
            "includePath": [
                "${workspaceFolder}/src", "${sdk}/**",
                "${workspaceFolder}/targets/pca10100/blank/config"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/src", "${sdk}/**",
                    "${workspaceFolder}/targets/pca10100/blank/config"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "${gcc}/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mabi=aapcs",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16"
            ],
            "forcedInclude": [
                "${workspaceFolder}/.vscode/undef.h"
            ]
        }
    ]
}
